\mode<presentation>

% Settings
\definecolor{orangeAgaetis}{RGB}{255,127,65}
\definecolor{paleOrange}{RGB}{255,204,153}
\definecolor{pinkAgaetis}{RGB}{223,25,149}
\definecolor{greenAgaetis}{RGB}{0,178,169}
\definecolor{blueAgaetis}{RGB}{64,126,201}
\definecolor{yellowAgaetis}{RGB}{241,180,52}
\definecolor{greyAgaetis}{RGB}{118,134,146}
\definecolor{blackAgaetis}{RGB}{51,63,72}

\setbeamercolor{footlinecolor}{bg=blackAgaetis, fg=white}
\setbeamercolor{headlinecolor}{bg=orangeAgaetis, fg=white}
\setbeamercolor{frametitle}{fg=white,bg=orangeAgaetis}

\setbeamercolor{itemize item}{fg=orangeAgaetis}
\setbeamertemplate{itemize item}[circle]
\setbeamerfont{itemize item}{size=\tiny}
\setbeamercolor{itemize subitem}{fg=greyAgaetis}
\setbeamertemplate{itemize subitem}[circle]
\setbeamerfont{itemize subitem}{size=\tiny}
\setbeamercolor{itemize subsubitem}{fg=greyAgaetis}
\setbeamertemplate{itemize subsubitem}[circle]
\setbeamerfont{itemize subsubitem}{size=\tiny}
\setbeamercolor{enumerate item}{fg=orangeAgaetis}
\setbeamercolor{enumerate subitem}{fg=orangeAgaetis}
\setbeamercolor{enumerate subsubitem}{fg=orangeAgaetis}


\mode<all>
